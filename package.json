{
  "name": "create-tag",
  "version": "1.0.2",
  "description": "GitHub Action to create tag",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.json",
    "release": "ncc build src/create-tag.ts -o action && git add -f action/",
    "clean": "rimraf -- action",
    "lint": "eslint **/*.ts"
  },
  "keywords": [
    "actions",
    "github",
    "tag",
    "create"
  ],
  "author": {
    "name": "Giao Ho",
    "email": "joutvhu@gmail.com",
    "url": "https://github.com/joutvhu"
  },
  "license": "MIT",
  "homepage": "https://github.com/joutvhu/create-tag#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joutvhu/create-tag.git"
  },
  "bugs": {
    "url": "https://github.com/joutvhu/create-tag/issues"
  },
  "dependencies": {
    "@actions/core": "^1.10.1",
    "@actions/github": "^6.0.0",
    "tslib": "^2.6.2"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.3",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^7.1.0",
    "@typescript-eslint/parser": "^7.0.2",
    "@vercel/ncc": "^0.38.1",
    "eslint": "^8.56.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-jest": "^27.9.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.1.2",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3"
  }
}
