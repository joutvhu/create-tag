{
  "name": "create-tag",
  "version": "1.0.1",
  "description": "GitHub Action to create tag",
  "scripts": {
    "build": "npm run clean && tsc --project tsconfig.json",
    "release": "ncc build src/create-tag.ts -o action && git add -f action/",
    "clean": "rimraf -- action",
    "lint": "eslint **/*.ts"
  },
  "keywords": [
    "actions",
    "github",
    "tag",
    "create"
  ],
  "author": {
    "name": "Giao Ho",
    "email": "joutvhu@gmail.com",
    "url": "https://github.com/joutvhu"
  },
  "license": "MIT",
  "homepage": "https://github.com/joutvhu/create-tag#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/joutvhu/create-tag.git"
  },
  "bugs": {
    "url": "https://github.com/joutvhu/create-tag/issues"
  },
  "dependencies": {
    "@actions/core": "^1.10.0",
    "@actions/github": "^5.1.1",
    "tslib": "^2.5.0"
  },
  "devDependencies": {
    "@tsconfig/recommended": "^1.0.2",
    "@types/node": "^16.18.12",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "@vercel/ncc": "^0.36.1",
    "eslint": "^8.33.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest": "^27.2.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^6.1.1",
    "jest": "^29.4.1",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
